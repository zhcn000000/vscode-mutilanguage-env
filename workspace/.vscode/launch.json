{
  "version": "2.0.0",
  "configurations": [
    {
      "type": "auto-debug",
      "name": "自动",
      "request": "launch",
      "map": {
        "*.cpp": "C/C++(msvc)",
        "*.c": "C/C++(msvc)",
        "*.py": "Python",
        "*.java": "Java(compile)",
        "*.js": "Nodejs",
        "*.ts": "Nodejs",
        "*.cs": "C#(dotnet)",
        "*.rb": "Ruby",
        "*.go": "Go",
        "*.rs": "Rust",
        "*.kt": "Kotlin",
        "*.swift": "Swift",
        "*.php": "PHP",
        "*.pl": "Perl",
        "*.sh": "Shell",
        "*.lua": "Lua",
        "*.sql": "SQL",
        "*.scala": "Scala",
        "*.pas": "Pascal",
        "*.r": "R",
        "*.m": "Matlab",
        "*.vb": "VisualBasic",
        "*.vbs": "VisualBasic",
        "*.ahk": "AutoHotkey",
        "*.bat": "Batch",
        "*.cmd": "Batch",
        "*.ps1": "PowerShell"
      }
    },
    {
      "type": "java",
      "name": "Java",
      "request": "launch",
      "args": [
        "<",
        "${workspaceFolder}\\test\\in.txt",
        ">",
        "${workspaceFolder}\\test\\out.txt"
      ],
      "mainClass": "${fileBasenameNoExtension}",
      "postDebugTask": "git提交"
    },
    {
      "type": "java",
      "name": "Java(compile)",
      "request": "launch",
      "args": [
        "<",
        "${workspaceFolder}\\test\\in.txt",
        ">",
        "${workspaceFolder}\\test\\out.txt"
      ],
      "mainClass": "${workspaceFolder}\\bin\\${fileBasenameNoExtension}.class",
      "preLaunchTask": "java编译",
      "postDebugTask": "git提交"
    },
    {
      "name": "C++(g++)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}\\bin\\${fileBasenameNoExtension}.exe",
      "args": [
        "<",
        "${workspaceFolder}\\test\\in.txt",
        ">",
        "${workspaceFolder}\\test\\out.txt"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "gdb.exe",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "C++编译(g++)",
      "postDebugTask": "git提交"
    },
    {
      "name": "C(gcc)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}\\bin\\${fileBasenameNoExtension}.exe",
      "args": [
        "<",
        "${workspaceFolder}\\test\\in.txt",
        ">",
        "${workspaceFolder}\\test\\out.txt"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "gdb.exe",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "C编译(gcc)",
      "postDebugTask": "git提交"
    },
    {
      "name": "C/C++(msvc)",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}\\bin\\${fileBasenameNoExtension}.exe",
      "args": [
        "<",
        "${workspaceFolder}\\test\\in.txt",
        ">",
        "${workspaceFolder}\\test\\out.txt"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "console": "integratedTerminal",
      "preLaunchTask": "C/C++编译(msvc)",
      "postDebugTask": "git提交"
    },
    {
      "name": "C#(dotnet)",
      "type": "coreclr",
      "request": "launch",
      "program": "${workspaceFolder}\\bin\\${fileBasenameNoExtension}.exe",
      "args": [
        "<",
        "${workspaceFolder}\\test\\in.txt",
        ">",
        "${workspaceFolder}\\test\\out.txt"
      ],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "integratedTerminal",
      "internalConsoleOptions": "openOnSessionStart",
      "launchBrowser": {
        "enabled": false
      },
      "preLaunchTask": "C#编译(dotnet)",
      "postDebugTask": "git提交"
    },
    {
      "name": "C#(mono)",
      "type": "mono",
      "request": "launch",
      "program": "${workspaceFolder}\\bin\\${fileBasenameNoExtension}.exe",
      "args": [
        "<",
        "${workspaceFolder}\\test\\in.txt",
        ">",
        "${workspaceFolder}\\test\\out.txt"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "preLaunchTask": "C#编译(mono)",
      "postDebugTask": "git提交"
    },
    {
      "name": "PHP",
      "type": "php",
      "request": "launch",
      "program": "${file}",
      "args": [
        "<",
        "${workspaceFolder}\\test\\in.txt",
        ">",
        "${workspaceFolder}\\test\\out.txt"
      ],
      "cwd": "${workspaceFolder}",
      "port": 9003,
      "postDebugTask": "git提交"
    },
    {
      "name": "Python",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "args": [
        "<",
        "${workspaceFolder}\\test\\in.txt",
        ">",
        "${workspaceFolder}\\test\\out.txt"
      ],
      "console": "integratedTerminal",
      "postDebugTask": "git提交"
    },
    {
      "name": "Nodejs",
      "type": "node",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": [
        "<",
        "${workspaceFolder}\\test\\in.txt",
        ">",
        "${workspaceFolder}\\test\\out.txt"
      ],
      "internalConsoleOptions": "neverOpen",
      "postDebugTask": "git提交"
    },
    {
      "name": "Go",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      //"program": "${workspaceFolder}\\bin\\${fileBasenameNoExtension}.exe",
      "program": "${file}",
      "env": {},
      "args": [
        "<",
        "${workspaceFolder}\\test\\in.txt",
        ">",
        "${workspaceFolder}\\test\\out.txt"
      ],
      "showLog": true,
      //"preLaunchTask": "Go编译",
      "postDebugTask": "git提交"
    },
    {
      "name": "Rust",
      "type": "lldb",
      "request": "launch",
      "program": "${workspaceFolder}\\bin\\${fileBasenameNoExtension}.exe",
      "args": [
        "<",
        "${workspaceFolder}\\test\\in.txt",
        ">",
        "${workspaceFolder}\\test\\out.txt"
      ],
      "preLaunchTask": "Rust编译",
      "postDebugTask": "git提交"
    },
    {
      "name": "Ruby",
      "type": "rdbg",
      "request": "launch",
      "script": "${file}",
      "useTerminal": true,
      "args": [
        "<",
        "${workspaceFolder}\\test\\in.txt",
        ">",
        "${workspaceFolder}\\test\\out.txt"
      ],
      "cwd": "${workspaceFolder}",
      "postDebugTask": "git提交"
    }
  ]
}
